{
    "name"       : "joomla-extensions/weblinks",
    "description": "The Open Source PHP Framework for creating complex Joomla extensions",
    "license"    : "GPL-2.0+",
    "config": {
        "platform": {
            "php": "5.5.33"
        }
    },
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "isacandrei/joomla-testing-robo",
                "version":"1",
                "source": {
                    "url": "https://github.com/isacandrei/joomla-testing-robo.git",
                    "type": "git",
                    "reference":"container-test"
                }
            }
        }
    ],
    "require"    : {
        "php": ">=5.3.10"
    },
    "require-dev": {
        "codeception/codeception"                    : "^2.2",
        "joomla-projects/selenium-server-standalone" : "v2.53.1",
        "fzaninotto/faker"                           : "^1.6",
        "Behat/Gherkin"                              : "^4.4.1",
        "joomla-projects/jorobo"                     : "dev-container-test",
        "isacandrei/joomla-testing-robo"             : "1",
        "joomla-projects/joomla-browser"             : "dev-container-test"
    },
    "minimum-stability"                              : "dev",
    "prefer-stable"                                  : true
}
